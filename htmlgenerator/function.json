{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "recordsjson",
      "type": "table",
      "tableName": "records",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 23 * * * *"
    },
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
