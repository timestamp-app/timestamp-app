{
  "name": "timestamp-app",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint .",
    "lint:report": "eslint . -f json -o eslint.json",
    "build": "tsc",
    "build:production": "npm run prestart && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host watch",
    "test": "jest --coverage",
    "deploy": "npm run build:production && func azure functionapp publish $npm_package_config_func_app_name --typescript",
    "deploy:dev": "npm run deploy --timestamp-app:func_app_name=timestampcollectordev",
    "deploy:prod": "npm run deploy --timestamp-app:func_app_name=timestampcollectorprod",
    "logs": "func azure functionapp logstream",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./eslint.json ./.scannerwork",
    "sonar": "sonar-scanner -D sonar.branch.name=$(git rev-parse --abbrev-ref HEAD)"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^1.0.1-beta1",
    "@types/node": "^14.14.27",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.1",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
